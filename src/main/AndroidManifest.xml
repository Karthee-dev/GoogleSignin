    <application>

        <provider
            android:authorities="${applicationId}.ContextProvider"
            android:name=".google.utils.ContextProvider"
            android:exported="false" />
        <activity android:name="arivista.googlelogin.google.GoogleAuthActivity"/>

    </application>
</manifest>
